    "eslint": "^8.48.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tornado-tools": "^0.0.2",
    "vitepress": "1.0.0-rc.10",
    "vue": "^3.3.4"
  },
  "private": true,
  "license": "MIT"
